chain:
    bech32_prefix: cudos
    modules: 
        - modules
        - messages
        - auth
        - bank
        - consensus
        - gov
        - slashing 
        - staking
        - history
        - pricefeed
        - cosmwasm
        - gravity
        - cudomint
        - nft
        - group
        - marketplace
        - cw20token
node:
    type: remote
    config:
        rpc:
            client_name: cudos-network
            address: http://host.docker.internal:26657
            max_connections: 20
        grpc:
            address: http://host.docker.internal:9090
            insecure: true
parsing:
    workers: 2
    listen_new_blocks: true
    parse_old_blocks: true
    parse_genesis: true
    start_height: 1
    fast_sync: false # when fast sync is set to true, ignores all previous block
    genesis_file_path: /usr/local/bdjuno/bdjuno/genesis.json
database:
    name: LocalBigDipperTest
    host: host.docker.internal
    port: 5432
    user: postgres
    password: Password
    schema: public
    max_open_connections: 10
    max_idle_connections: 10
logging:
    level: debug
    format: text
telemetry:
    port: 5000
pricefeed:
    tokens:
        - name: Cudos
          units:
            - denom: cudos
              exponent: 0
              price_id: cudos
workers:
    - name: fix_blocks_worker
      interval: 60m
    - name: migrate_nfts_worker
      interval: 1m
    - name: blocks_monitoring_worker
      interval: 30s
cudomint:
    stats_service_url: http://127.0.0.1:3000
verified_contracts_subscription:
    project_id: multisig-firestore-1
    sub_id: my-sub
